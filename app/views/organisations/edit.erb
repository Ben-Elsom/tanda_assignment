<h1>Edit page</h1>
<%= @organisation.name %>

 <%= form_with model: @organisation do |form| %>
        <label>Name</label> 
        <%= form.text_field :name%><br/>
        <label>Hourly Rate</label> 
        <%= form.number_field :hourly_rate%><br/>
        <%= form.submit "Update organisation"%>
<% end %>

